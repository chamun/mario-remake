<?xml version="1.0" encoding="UTF-8"?>
<!--Format:
name   => name of the animation sequence
start  => starting frame
end    => ending frame
loop   => looping animation (default)
-->
<animation name="monkey">
    <sequence name="standing"  start="0" end="0" loop="false" />
    <sequence name="walking" start="0" end="1" loop="true" />
    <sequence name="standing-holding"  start="2" end="2" loop="false" />
    <sequence name="moving-holding" start="2" end="3" loop="true" />
    <sequence name="kicking"  start="4" end="4" loop="false" />
    <sequence name="jumping"  start="5" end="5" loop="false" />
    <sequence name="falling"  start="6" end="6" loop="false" />
    <sequence name="breaking" start="7" end="7" loop="false" />
    <sequence name="ducking"  start="8" end="8" loop="false" />
    <sequence name="ducking-holding"  start="13" end="13" loop="false" />
    <sequence name="running"  start="9" end="10" loop="true" />
    <sequence name="jumping-running" start="11" end="11" loop="false" />
    <sequence name="looking-up"  start="12" end="12" loop="false" />
	<!--
    <sequence name="looking-up-holding"  start="13" end="13" loop="false" />
	-->
</animation>

